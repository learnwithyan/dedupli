{
  "name": "dedupli",
  "displayName": "DeDupli remove duplicates",
  "description": "Retain distinct lines of text, eliminating any duplicates from the current selection. Additionally, implement a command for randomizing the order of the presently selected lines.",
  "version": "1.0.0",
  "publisher": "learnwithyan",
  "icon": "images/logo_128.png",
  "bugs": {
    "url": "https://github.com/learnwithyan/dedupli",
    "email": "learnwithyan@hotmail.com"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "dedupli.remDuplicates",
        "title": "DeDupli - remove duplicates from selected",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "dedupli.shuffle",
        "title": "DeDupli - shuffle lines from selected",
        "icon": "$(source-control)"
      }
    ]
  },
  "configuration": {
    "title": "Dedupli Configarations",
    "properties": {
      "dedupli.lineBreak": {
        "type": "string",
        "default": "\n",
        "scope": "resource",
        "description": "Description of your setting"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/learnwithyan/dedupli.git"
  }
}
